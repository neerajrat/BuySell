<h1>New Category</h1>

<%= form_for @category do |form| %>
    <div>
      <%= form.label :category_name %><br>
      <%= form.text_field :category_name %>
      <% @category.errors.full_messages_for(:category_name).each do |message| %>
      <div><%= message %></div>
      <% end %>
    </div>
  


    <div>
      <%= form.submit "submit" %>
    </div>

  <% end %>

